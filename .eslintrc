{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "plugins": [
    "react"
  ],
  "globals": {
    "inkdrop":  true,
    "debug": true
  },
  "rules": {
    "no-useless-escape": 0,
    "no-return-assign": 0,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}